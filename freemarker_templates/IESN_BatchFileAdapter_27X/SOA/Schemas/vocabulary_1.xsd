    <xsd:simpleType name="roleStatusNormalType">
        <xsd:annotation>
            <xsd:documentation>This code set is used as a modifier for the provider role and territorial authority.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="normal"/>
            <xsd:enumeration value="active"/>
            <xsd:enumeration value="cancelled"/>
            <xsd:enumeration value="pending"/>
            <xsd:enumeration value="suspended"/>
            <xsd:enumeration value="terminated"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="genderType">
        <xsd:annotation>
            <xsd:documentation>This code set is used to specify the gender of a person.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Female"/>
            <xsd:enumeration value="Male"/>
            <xsd:enumeration value="Unknown"/>
            <xsd:enumeration value="Ambiguous"/>
            <xsd:enumeration value=""/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ynEnumType">
        <xsd:annotation>
            <xsd:documentation>Enum for Boolean (Y, N)</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Y"/>
            <xsd:enumeration value="N"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="communicationType">
        <xsd:annotation>
            <xsd:documentation>Communication type like - Phone, Fax, Email etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Phone"/>
            <xsd:enumeration value="Mobile"/>
            <xsd:enumeration value="Email"/>
            <xsd:enumeration value="Fax"/>
            <xsd:enumeration value="Office"/>
            <xsd:enumeration value="Billing"/>
            <xsd:enumeration value="Other"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="locationType">
        <xsd:annotation>
            <xsd:documentation>Location type like - Home, Office etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Home"/>
            <xsd:enumeration value="Office"/>
            <xsd:enumeration value="Other"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="resultStatusType">
        <xsd:annotation>
            <xsd:documentation>Transaction status, like SUCCESS, FAILURE, UNKNOWN</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Success">
                <xsd:annotation>
                    <xsd:documentation>Receiving application successfully processed message.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Failure">
                <xsd:annotation>
                    <xsd:documentation>Receiving application found error in processing message. Sending error response with additional error detail information.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Unknown">
                <xsd:annotation>
                    <xsd:documentation>Receiving application failed to process message for reason unrelated to content or format. Original message sender must decide on whether to automatically send message again.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="resultCodeType">
        <xsd:annotation>
            <xsd:documentation>Result Codes like HIPAA, LEGACY (NEHEN) and MITS</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="HIPAA"/>
            <xsd:enumeration value="LEGACY"/>
            <xsd:enumeration value="SDI"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="actionToRecoverType">
        <xsd:annotation>
            <xsd:documentation>Action To Recover like RETRY, RETRY LATER, PERMANENT FAILURE</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Retry"/>
            <xsd:enumeration value="RetryLater"/>
            <xsd:enumeration value="PermanentFailure"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="dateOfBirthInterpretationTypeEnumType">
        <xsd:annotation>
            <xsd:documentation>Exactness of the Date Of Birth E=Exact, D=Day not known, M=Month not known</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="E"/>
            <xsd:enumeration value="D"/>
            <xsd:enumeration value="M"/>
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>
